{
  "name": "fabricjs-react",
  "version": "2.3.0",
  "description": "support fabricjs from react",
  "author": "Alejandro Soto",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.declaration.json",
    "prepare": "npm run build",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/fabricjs-react.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fabricjs-react.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "fabric": "^5 || ^6 || ^7",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@types/fabric": "^5.3.7",
    "@types/node": "^20.19.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "path": "^0.12.7",
    "rollup-plugin-node-externals": "^7.1.3",
    "typescript": "^4.6.4",
    "vite": "^7.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asotog/fabricjs-react.git"
  },
  "bugs": {
    "url": "https://github.com/asotog/fabricjs-react/issues"
  },
  "homepage": "https://github.com/asotog/fabricjs-react#readme",
  "keywords": [
    "fabricjs",
    "fabric.js",
    "react",
    "canvas",
    "react-hooks",
    "fabricjs-react"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
}
